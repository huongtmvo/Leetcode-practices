   "source": [
    "class Solution:\n",
    "    def missingNumber(self, nums: List[int]) -> int:\n",
    "        n = len(nums)\n",
    "        return sum([i for i in range(n+1)]) - sum(nums)\n",
    "    "
   ]
  }
