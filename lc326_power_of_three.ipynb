   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def isPowerOfThree(self, n: int) -> bool:\n",
    "        if n < 1: return False\n",
    "        if n == 1: return True\n",
    "        if n % 3 != 0: return False\n",
    "        \n",
    "        return self.isPowerOfThree(n//3)"
   ]
